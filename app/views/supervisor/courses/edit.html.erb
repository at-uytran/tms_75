<% provide :title, t(".title") %>
<% provide :button_text, t(".btn_submit") %>
<div class="col-md-4">
  <h3><%= t ".title" %></h3>
  <div class="w3_login_module">
    <div class="module form-module">
      <%= render "form" %>
    </div>
  </div>
</div>
<% if @course.init? %>
  <div class="col-md-6 add-remove-subject">
    <h3><%= t ".add_remove_sb" %></h3>
    <ul>
      <%= render partial: "add_subject", collection: @all_subjects, as: :subject %>
    </ul>
  </div>
<% end %>
