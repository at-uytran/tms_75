<%= form_for [:supervisor, @task] do |f| %>
  <%= render "shared/error_messages", object: f.object %>

  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>

  <%= f.label :description %>
  <%= f.text_area :description, class: "form-control",rows: Settings.area_size %>


  <%= f.submit yield(:button), class: "btn btn-primary" %>
<% end %>
